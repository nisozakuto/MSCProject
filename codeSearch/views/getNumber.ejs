<%- include('./partials/boilerplate') %> <%- include('./partials/nav') %>
<main id="getNumbers">
  <div class="list-group">
    <h4 class="list-group-item-heading"><%= results.length %></h4>
  </div>

  <div>
    <table>
      <tr>
        <th>Index</th>
        <th>Number</th>
        <th>Result</th>
      </tr>
      <% for (var i = 0; i < results.length; i++) { %>
      <tr>
        <td><%= i %></td>
        <td><%= results[i][0] %></td>
        <td><%= results[i][1] %></td>
        <td><%= results[i][2] %></td>
      </tr>
      <% } %>
    </table>
  </div>
</main>
